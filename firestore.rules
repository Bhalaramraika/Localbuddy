
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: This rule is for development only and is insecure.
    // It allows any authenticated user to read and write to the entire database.
    // You should secure your database before going to production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
